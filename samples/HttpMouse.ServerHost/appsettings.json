{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:2880"
      }
    }
  },
  "HttpMouse": {
    "DefaultKey": "httpmouse",
    "DefaultCluster": {
      "HttpClient": {
        "MaxConnectionsPerServer": 10,
        "DangerousAcceptAnyServerCertificate": true
      },
      "HttpRequest": {
        "Timeout": "00:02:00"
      }
    }
  },
  "Fallback": {
    "StatusCode": 503,
    "ContentType": "application/problem+json",
    "ContentFile": "fallback.json"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Yarp.ReverseProxy": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  }
}